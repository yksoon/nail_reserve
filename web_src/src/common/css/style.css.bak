/* btn */
a.login {display:inline-block;width: 104px;height: 36px;line-height: 36px;background: #0047A0;color: #fff;font-size: 12px;font-weight: 500;text-align: center;border-radius: 50px;}
.btn_box {margin:0 auto;text-align:center;margin:60px 0 0;}
a.mainbtn {display:inline-block;width: 280px;height: 68px;line-height: 68px;font-size: 21px;font-weight: 500;text-align: center;border-radius: 50px;margin: 0 20px 0 0 ;transition:all 0.3s;}
a.mainbtn:last-child {margin: 0;}
a.mainbtn.btn01 {background: #0047A0;color: #fff;}
a.mainbtn.btn01:hover {background: #003679;}
a.mainbtn.btn02 {background: #EFEFEF;color: #A6A6A6;}
a.mainbtn.btn02:hover {background: #ddd;}
a.subbtn {display:inline-block;width:auto;min-width:146px;padding: 0 38px;height: 48px;text-align: center;border-radius: 10px;line-height: 48px;}
a.subbtn.on {border: 1px solid #0047a0;color: #0047a0;transition:all 0.3s;}
a.subbtn.on:hover {background: #0047a0;color:#fff;}
a.subbtn.off {border: 1px solid #ddd;color: #ddd;}
a.subbtn.hold {border: 1px solid #efefef;background: #efefef;color: #bdbdbd;}
a.subbtn.hold:hover {border: 1px solid #efefef;background: #efefef;color: #bdbdbd;}
a.backbtn {display:inline-block;height: 53px;line-height: 53px;font-size: 16px;text-align: center;border-radius: 50px;transition:all 0.3s;border: 1px solid #ddd;padding: 0 60px;color: #a8a8a8;}
a.backbtn:hover {color: #616161;border: 1px solid #818181;}
a.hotel_btn {display:inline-block;width: 220px;height: 53px;line-height: 53px;font-size: 16px;text-align: center;border-radius: 50px;margin: 0 20px 0 0 ;transition:all 0.3s;}
.file {display:none;}
.file_btn {display:inline-block;width:auto;min-width:146px;padding: 0 38px;height: 48px;text-align: center;border-radius: 10px;line-height: 48px;border: 1px solid #ddd;color: #818181;cursor: pointer;}

select {border: 1px solid #ddd;height:48px;line-height:48px;font-size: 18px;color: #333;border-radius: 10px;padding: 0 20px;cursor:pointer}
input::placeholder {color: #a6a6a6;}
input.input {border: 1px solid #ddd;height:48px;line-height:48px;font-size: 18px;color: #333;border-radius: 10px;padding: 0 20px;display:inline-block;min-width:80px;margin:0 10px 0 0 ;}
input.input.hold {background: #efefef;color: #bdbdbd;}
input.input:last-child {margin:0;}
input.login {border: 1px solid #818181;height:36px;line-height:36px;font-size: 12px;color: #333;border-radius: 20px;padding: 0 20px;}
label.checkbox {display:flex;width:30px;height:30px;border: 1px solid #bdbdbd;border-radius: 50%;text-align:center;align-items: center;justify-content: center;cursor: pointer;}
input[type="checkbox"]:checked + label.checkbox {background:#0047a0;border: 1px solid #0047a0;}
input[type="checkbox"]:checked + label.checkbox svg {fill:#fff;}

