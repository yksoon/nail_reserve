/* alert */
.alert_wrap {display:block;position:fixed;top:0;left:0;width:100%;height:100vh;overflow:hidden;background: rgba(0,0,0,0.1);z-index:9999;}
.alert_wrap .alert {position:absolute;top:50%;left:50%;transform:translate(-50%, -50%); box-shadow: 5px 5px 15px rgba(0,0,0,0.1);background:#fff;padding:40px 50px;max-height:840px;overflow-y:scroll;z-index:999;border-radius: 5px;z-index:10;width:600px;max-height:500px;text-align: center;z-index:100;}
.alert.modal_big {width:800px;max-height:800px;text-align: center;}
.alert h3 {font-size:24px;font-weight: 700;}
.alert p {font-size:21px;font-weight: 500;margin:12px 0 0;}
.alert .noti_icon {text-align:center;display: block;margin:0 0 24px;}
.alert .noti_icon img {width: 80px;}
.alert{ -ms-overflow-style: none; } .alert::-webkit-scrollbar{ display:none; }
.alert span.alert_icon {color:#fff;font-weight: 900;display:inline-block;width:24px;height:24px;background: #dd0000;border-radius: 50%;margin:0 0 10px;line-height: 24px;}
.alert span.confirm_icon {color:#fff;font-weight: 900;display:inline-block;width:24px;height:24px;background: #2f81e9;border-radius: 50%;margin:0 0 10px;line-height: 24px;}

