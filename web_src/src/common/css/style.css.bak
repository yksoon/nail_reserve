.my_inq .i_status {background: #a6a6a6; color: #fff;font-size: 16px;line-height: 26px;padding: 0 16px;border-radius: 50px;display:inline-block;margin: 0 0 0 10px;}
.my_inq p {margin:0 0 20px;overflow: hidden;text-overflow: ellipsis;white-space: nowrap;}
.my_inq p.date {color: #9e9e9e;margin:0 0 10px;}
.my_inq .i_status.end {background: #0047a0;}

.my_inq_view .i_status {background: #a6a6a6; color: #fff;font-size: 16px;line-height: 26px;padding: 0 16px;border-radius: 50px;display:inline-block;margin: 0 0 0 10px;}
.my_inq_view .i_status.end {background: #0047a0;}

.my_fix_wrap {float: left;width:410px;}
.fixed_menu {position:fixed;width:410px;}
.my_quick {padding: 40px;border: 1px solid #ddd;border-radius: 20px;margin:0 0 32px}
.my_quick .name {display:flex; align-items: center;margin: 0 0 28px 0;flex-wrap: wrap;}
.my_quick .name h3 {font-size: 32px;font-weight: 700;color: #0047a0;margin:0 18px 0 0;}
.my_quick .name p.point {color: #0047a0;font-size: 24px;font-weight: 700;}
.my_quick .name p.point span {display: inline-block;margin:0 5px 0 0;vertical-align: middle;}
.my_quick .name p.point span img {margin:-4px 0;}
.my_quick .my_list .flex {align-items: flex-start;gap:40px;}
.my_quick .my_list ul {}
.my_quick .my_list ul li a {display: inline-block; color: #333;font-size: 21px;font-weight: 700;margin: 12px 0 0;}
.my_quick .my_list ul li a span{color: #a6a6a6;}
.my_quick .my_list ul li a:hover {color: #0047a0;}

.add_banner li {margin:0 0 10px;border-radius: 20px;overflow: hidden;height:140px;}
.add_banner li a {display:block;width:100%;height:100%;}
.add_banner li a img {display:block;height:100%;}

/* 마이페이지 - 회원정보 수정 */
.modify_form {text-align: center;}
.modify_form h5 {margin: 0 0 20px;}
.change_profile_wrap {width: 100%;}
.change_profile_wrap .change_profile_thumb {display:block;width:160px;height:160px;border-radius: 50%;border: 1px solid #ddd;}
.change_profile_wrap .change_profile_thumb img {width: 100%;}
.change_profile_wrap .imglist {padding:30px 10px;border: 1px solid #ddd;border-radius: 10px;margin: 0 0 5px;}
.change_profile_wrap .imglist img {width: 56px; border-radius: 50%; border: 1px solid #e2e2e2;margin: 0 5px 0 0;cursor: pointer;}
.change_profile_wrap input[type="text"] {width: calc( 100% - 160px );}


/* 호텔 */
/* 호텔리스트 */
.hotel_list ul li {border: 1px solid #ddd;padding: 40px;display:flex;border-radius: 50px;}
.hotel_list ul li span.hotel_list_thumb {width: 745px;height: 450px;overflow:hidden;border-radius: 50px;}
.hotel_list ul li span.hotel_list_thumb img {width: 100%;}
.hotel_list ul li .hotel_list_txt {width: calc( 100% - 745px ) ;padding:0 0 0 30px;}
.hotel_list ul li .hotel_list_txt h3 {font-size: 32px;font-weight: 700;color: #000;}
.hotel_list ul li .hotel_list_txt h5 {font-size: 21px;font-weight: 600;color: #616161;margin:10px 0 20px;}
.hotel_list ul li .hotel_list_txt .hotel_venue p{font-size: 21px;font-weight: 500;}
.hotel_list ul li .hotel_list_txt .hotel_venue p span{display:inline-block;vertical-align:middle;margin:0 10px 0 0;}
.hotel_list ul li .hotel_list_txt .hotel_intro {color: #333;margin:10px 0 20px;margin:28px 0 40px;}
.hotel_list ul li .hotel_list_txt .hotel_intro .hotel_intro_more {display:inline-block;color: #818181;cursor:pointer;}