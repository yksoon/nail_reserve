.guideline .boxing .steps li.c02 p {background:url('/public/img/web/sub/step02.png') center center no-repeat;}
.guideline .boxing .steps li.c03 p {background:url('/public/img/web/sub/step03.png') center center no-repeat;}
.guideline .boxing .steps li.c04 p {background:url('/public/img/web/sub/step04.png') center center no-repeat;}
.guideline .boxing .steps li.c05 p {background:url('/public/img/web/sub/step05.png') center center no-repeat;}
.guideline .boxing .steps li.c06 p {background:url('/public/img/web/sub/step06.png') center center no-repeat;}
.guideline .boxing dl dt {font-size:20px;font-weight: 900;color:#000;margin:25px 0 10px 0;}
