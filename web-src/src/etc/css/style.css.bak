@charset "utf-8";

body {min-width:1300px;max-width: 1920px;margin:0 auto;}
#header  {position:absolute;width:100%; height:130px; z-index:999;}
#header_content {width:1300PX;height:130px; margin:0 auto;}
#header_content .logo {position:absolute; top:33px; left:0;z-index:1000;}
#header_content .logo a{display:inline-block;}




/* gnb */
#gnb {position:relative;top:0; height:130px;display: flex;}
#gnb ul {display:flex;justify-content:flex-end; width:1300px; margin: 0 auto;text-align:center; align-items: center;}
#gnb ul:after {content:" "; display:block; clear:both;}
#gnb ul li {height:130px;line-height:130px;margin: 0 0 0 50px; text-align:center; box-sizing:border-box;position:relative;}
#gnb ul li > a {display:block; color:#ffffff; font-size:19px;font-weight: 900;}
#gnb ul li:hover > a  {color:#ffa5c8;}
#gnb ul li:hover .submenu { display:block;}

#gnb ul li .submenu {display:none;position:absolute; top:100px; left:50%; width:250px; margin-left:-125px; line-height:40px;  padding:20px 0;animation:menuDown 0.3s linear;
	background: rgb(124,29,146);
	background: linear-gradient(325deg, rgba(124,29,146,1) 0%, rgba(235,38,115,0.9) 100%);
	border-radius:5px;
	-webkig-border-radius:5px;
	-moz-border-radius:5px;

}
#gnb ul li .submenu a {display:block;color:#fff; font-size:17px;letter-spacing:-0.04em;}
#gnb ul li .submenu a:hover, #gnb ul li .submenu a:focus {color:#ffcee1;}

@keyframes menuDown{
	0% {
        opacity:0; 
		margin-left:-145px;
    }

    100% {
        opacity:1; 
		margin-left:-125px;
    }
}


/** mobile_none **/
.mobile_top {display:none;}
.tops {display:none;}
#nav {display:none;}


#mainvisual {position:relative;z-index:1; width:100%; height:860px; background:url('/public/img/web/main/mainvisual.png') center 0 no-repeat;}
#mainvisual  .main_txt {width:1300px; margin:0 auto;position:relative; top:200px;}
#mainvisual  .main_txt_in {width:650px; text-align: center;}
#mainvisual  .main_txt_in h2{margin:0 0 20px 0;}
#mainvisual  .main_txt_in > h3{font-size:24px; color:#fff; letter-spacing: 0.23em;margin:0 0 30px 0;}
#mainvisual  .main_txt_in > .date{color:#fff;line-height:40px;font-size: 28px; font-weight: 600;text-align: center; padding:0 0 50px;}
#mainvisual  .main_txt_in > .date .pink{ color:#fa77a9;font-size: 36px; font-weight: 900;}


#mainvisual .main_txt .ob_img {position:absolute; top:-200px; right:0;}
#container {width:100%; }




/* section01 */
.section01 {width:100%; position:relative;margin:-190px auto 0;padding:250px 0 100px;background:url('/public/img/web/main/sec01_bg.jpg') center center no-repeat;background-size:cover;}
.section01 .sec01_in {width:1300px; margin: 0 auto;}
.section01 h3 {text-align: center;font-size:34px; color:#81109a;padding:0 0 25px 0;font-weight: 900;;}
.section01 .txt{font-size:17px; line-height:27px;color:#111111;text-align: center;padding:0 0 50px 0;}
.section01 .txt b{font-size:20px;}
.section01 .hostbox {display:flex;justify-content: center;align-items: center;margin:30px 0 0 0;}
.section01 .hostbox span.tit{ display:inline-block; padding:5px 0; width:120px; text-align:center;background-color: #f0f0f0;color:#81109a;font-size:15px;font-weight: 900;
	border-radius:40px;
	-webkit-border-radius:40px;
	-moz-border-radius:40px;
	margin:0 30px 0 0 ;
}
.section01 .hostbox span.logobox {margin:0 50px 0 0;}

.section01 .overview {margin:0 auto 80px;width:850px;}
.section01 .overview li{margin:5px 0;display:flex;align-items:center;font-size:20px; font-weight:600; color:#111;line-height:45px;letter-spacing: -0.02em;}
.section01 .overview li span{display:inline-block; width:170px; height:40px; line-height: 40px; font-size:16px;text-align: center; border-radius:40px;background-color:#81109a;color:#fff;font-weight: 900;margin-right:40px;}
.section01 .overview li:nth-child(odd) span {background-color:#ee508c;}
.section01 .overview li p{width:calc(100% - 210px);word-break: keep-all;}


.section01  .free {width:510px; height:auto; box-sizing:border-box; position:absolute;z-index:1;top:-120px;padding:8px;margin-left:70px;
	background: rgb(255,13,75);
	background: linear-gradient(345deg, rgba(255,13,75,1) 0%, rgba(249,201,34,1) 100%);
	border-radius:5px;
	-webkit-border-radius:5px;
	-moz-border-radius:5px;
	box-shadow: 5px 5px 10px rgba(0,0,0,0.3);
	-webkit-box-shadow: 5px 5px 10px rgba(0,0,0,0.3);
	-moz-box-shadow: 5px 5px 10px rgba(0,0,0,0.3);
}

.section01 .free .free_in{background-color:#fff;padding:10px 20px 10px;}
.section01 .free .free_in .tit{position:absolute;top:-50px; left:-64px;animation: shake .5s infinite alternate;}


@keyframes shake {
	from {
		transform:rotate(-3deg);
	}
  
	to {
		transform:rotate(0);
	}
  }


.section01 .free .free_in h3{width:100%;text-align: center;font-size: 28px; color:#ff004b; font-weight:900;letter-spacing: -0.01em;padding:15px 0 7px;
    background:url('/public/img/web/main/free_st.png') 75% 0 no-repeat;
}
.section01 .free .free_in ul li {position:relative;padding: 20px 0 20px 100px;border-bottom:1px solid #ddd;text-align:left;line-height:20px;font-size:20px;font-weight:900;color:#000;}
.section01 .free .free_in ul li:last-child{bordeR:none;}
.section01 .free .free_in ul li b{color:#ff004b;}
.section01 .free .free_in ul li b.long {font-size:17px;}
.section01 .free .free_in ul li .date {position:absolute; left:0; top:20px;display:inline-block;width:85px; height: 23px; line-height: 23px; background-color:#ff004b;
	text-align:center;margin:0 15px 0 0;color:#fff;font-size:17px;
	border-radius: 23px;
	-webkit-border-radius: 23px;
	-moz-border-radius: 23px;
}
.section01 .free .free_in ul li .icon {position:absolute; top:23%; right:20px;}



/* section02 */
.section02 {width:100%;height:130px; background:url('/public/img/web/main/sec012_bg.jpg') center 0 repeat-y;}
.section02 .sec02_in {width:1300px; margin:0 auto; height:130px; display:flex;}
