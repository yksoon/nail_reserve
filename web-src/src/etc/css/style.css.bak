/* PROGRAM */
.program .boxing {margin:0 0 60px 0;}
.program .tabbox{margin:0 0 50px 0; text-align: center;}
.program .tabbox a{ display:inline-block;height:60px; line-height:60px; width:300px; border:1px solid #59047f;color:#59047f;border-radius:60px;font-size:20px;font-weight:600;margin:0 6px;}
.program table.de_tb thead tr th{background-color:#5e0875;padding:20px 0;font-size:19px;font-weight: 900;color: #fff;;}
.program table.de_tb thead tr th:first-child {border-radius:62px 0 0 62px;}
.program table.de_tb thead tr th:last-child {border-radius:0 62px 62px 0;}
.program table.de_tb tbody tr td{border-bottom:1px solid #ccc; padding:25px 0; text-align: center;font-size:18px;color:#000; font-weight:900;}
.program table.de_tb tbody tr td.time{color:#5e0875; font-weight: 400;}

.program table.glance_tb  {border-spacing: 5px;border-collapse: separate;}
.program table.glance_tb thead tr th.pur {background-color:#5e0875;padding:20px 0;font-size:22px;font-weight: 900;color: #fff;
	border-radius:65px;
	-webkit-border-radius:65px;
	-moz-border-radius:65px;
}
.program table.glance_tb thead tr.gray th {background-color:#f4f4f4; padding:22px 5px; font-size:19px; color:#5e0875;
	border-radius:10px;
	-webkit-border-radius:10px;
	-moz-border-radius:10px;
}
.program table.glance_tb tbody tr td {height:50px;font-size:19px;font-weight:900;}
.program table.glance_tb tbody tr td.time {padding: 15px 5px; border-bottom:1px solid #ddd;text-align:center;color:#5e0875;font-size:17px;font-weight:400;}
.program table.glance_tb tbody tr td.normal {padding: 15px 5px; border-bottom:1px solid #ddd;text-align:center;color:#000;font-weight:400;}
.program table.glance_tb tbody tr td > div {height:100%;text-align:center; display: flex;align-items: center;justify-content: center;
	-webkit-border-radius: 10px;
	-moz-border-radius: 10px;
}
.program table.glance_tb tbody tr td .c01 {background-color:#fde2ec;}
.program table.glance_tb tbody tr td .c02 {background-color:#f6e2fd;}
.program table.glance_tb tbody tr td .c03 {background-color:#e2e8fd;}
.program table.glance_tb tbody tr td .c04 {background-color:#f9fde2;}
.program .noti {text-align:right; font-size:16px; color:Red;margin:10px 0 0 0; line-height:22px;}



